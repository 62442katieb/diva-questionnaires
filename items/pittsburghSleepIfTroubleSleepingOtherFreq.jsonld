{
    "@context": [
        "https://raw.githubusercontent.com/ReproNim/schema-standardization/master/contexts/generic"
    ],
    "@type": "https://raw.githubusercontent.com/ReproNim/schema-standardization/master/schemas/Field",
    "@id": "pittsburghSleepIfTroubleSleepingOtherFreq",
    "skos:prefLabel": "Trouble Sleeping Other Freq",
    "skos:altLabel": "PSQI5k",
    "schema:description": "Trouble Sleeping, Other Frequency",
    "schema:schemaVersion": "0.0.1",
    "schema:version": "0.0.1",
    "question": "And how often have you had trouble sleeping because of this?",
    "ui": {
        "inputType": "radio",
        "allow": [
            "autoAdvance"
        ]
    },
    "responseOptions": {
        "minValue": 1,
        "maxValue": 4,
        "requiredValue": true,
        "choices": [
            {
                "schema:name": "1 - Not during the past month",
                "schema:value": 1
            },
            {
                "schema:name": "2 - Less than once a week",
                "schema:value": 2
            },
            {
                "schema:name": "3 - Once or twice a week",
                "schema:value": 3
            },
            {
                "schema:name": "4 - Three or more times a week",
                "schema:value": 4
            }
        ]
    }
