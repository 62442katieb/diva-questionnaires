{
    "@context": "https://raw.githubusercontent.com/ReproNim/schema-standardization/master/contexts/generic",
    "@type": "https://raw.githubusercontent.com/ReproNim/schema-standardization/master/schemas/Field",
    "schema:schemaVersion": "0.0.1",
    "schema:version": "0.0.1",
    "@id": "BISBASfindTime",
    "skos:prefLabel": "BIS/BAS filler question about finding time to do things like get a hair cut",
    "skos:altLabel": "filler3",
    "schema:description": "asks participant if they have a hard time finding time to run errands",
    "question": "It is hard for me to find the time to do things such as get a haircut.",
    "ui": {
        "inputType": "radio",
        "allow": [
            "autoAdvance"
        ]
    },
    "responseOptions": {
        "minValue": 1,
        "maxValue": 4,
        "requiredValue": true,
        "choices": [
            {
                "schema:name": "1 - very true for me",
                "schema:value": 1
            },
            {
                "schema:name": "2 - somewhat true for me",
                "schema:value": 2
            },
            {
                "schema:name": "3 - somewhat false for me",
                "schema:value": 3
            },
            {
                "schema:name": "4 - very false for me",
                "schema:value": 4
            }
        ]
    }
